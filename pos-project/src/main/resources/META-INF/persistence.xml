<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" version="2.0">

    <persistence-unit name="bot">
        <provider>org.apache.openjpa.persistence.PersistenceProviderImpl</provider>
        <jta-data-source>java:/PostgresDS</jta-data-source>
        <class>com.pos.common.model.OrderMain</class>
        <class>com.pos.common.model.BaseEntity</class>
        <properties>
            <property name="openjpa.DynamicEnhancementAgent" value="true"/>
            <property name="openjpa.RuntimeUnenhancedClasses" value="supported"/>
            <property name="openjpa.Log" value="log4j"/>
            <!--<property name="openjpa.ConnectionFactoryProperties" value="PrintParameters=True, PrettyPrint=true"/>-->
        </properties>
    </persistence-unit>
</persistence>
